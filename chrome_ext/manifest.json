{
  "permissions": [
    "tabs",
    "bookmarks",
    "<all_urls>",
    "unlimitedStorage"
  ],
  "manifest_version": 2,
  "name": "AES EXT",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
 },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
